<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      考古发掘信息<include file="org/bkgd/hrmis/relic/web/conf/HrmisRelicArchWeb.xml"></include> -->
<struts>
	<!-- 考古发掘信息 -->
	<package name="HrmisRelicArch_Ajax_code" extends="json-default">
		<!-- 考古发掘信息查询 -->
		<action name="HrmisRelicArchqueryAll" method="queryall"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicArchQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 考古发掘信息增加 -->
		<action name="HrmisRelicArchaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicArchQuery">
			<result type="json"></result>
		</action>
		<!-- 考古发掘信息删除 -->
		<action name="HrmisRelicArchdeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicArchQuery">
			<result type="json"></result>
		</action>
		<!-- 考古发掘信息修改 -->
		<action name="HrmisRelicArcheditCommit" method="editSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicArchQuery">
			<result type="json"></result>
		</action>
	</package>
	<package name="HrmisRelicArch_Jsp_code" extends="struts-default">
		<!-- 考古发掘信息详细【修改、浏览】 -->
		<action name="HrmisRelicArchshow" method="view"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicArchQuery">
			<result>/hrmis/relic/pHrmisRelicArchEntity.jsp</result>
		</action>
		<!-- html加载考古发掘信息控制台 -->
		<action name="HrmisRelicArch_ACTION_CONSOLE">
			<result>/hrmis/relic/pHrmisRelicArchLayout.jsp</result>
		</action>
		<!-- ajax加载考古发掘信息选择页面 -->
		<action name="HrmisRelicArch_ACTION_ALERT" >
			<result>/hrmis/relic/pHrmisRelicArchGrid.jsp</result>
		</action>
	</package>
</struts>