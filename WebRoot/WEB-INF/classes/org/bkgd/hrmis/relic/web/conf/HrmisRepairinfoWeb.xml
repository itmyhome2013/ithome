<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      修复信息<include file="org/bkgd/hrmis/relic/web/conf/HrmisRepairinfoWeb.xml"></include> -->
<struts>
	<!-- 修复信息 -->
	<package name="HrmisRepairinfo_Ajax_code" extends="json-default">
		<!-- 修复信息查询 -->
		<action name="HrmisRepairinfoqueryAll" method="queryall"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRepairinfoQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 修复信息增加 -->
		<action name="HrmisRepairinfoaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRepairinfoQuery">
			<result type="json"></result>
		</action>
		<!-- 修复信息删除 -->
		<action name="HrmisRepairinfodeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRepairinfoQuery">
			<result type="json"></result>
		</action>
		<!-- 修复信息修改 -->
		<action name="HrmisRepairinfoeditCommit" method="editSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRepairinfoQuery">
			<result type="json"></result>
		</action>
	</package>
	<package name="HrmisRepairinfo_Jsp_code" extends="struts-default">
		<!-- 修复信息详细【修改、浏览】 -->
		<action name="HrmisRepairinfoshow" method="view"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRepairinfoQuery">
			<result>/hrmis/relic/pHrmisRepairinfoEntity.jsp</result>
		</action>
		<!-- html加载修复信息控制台 -->
		<action name="HrmisRepairinfo_ACTION_CONSOLE">
			<result>/hrmis/relic/pHrmisRepairinfoLayout.jsp</result>
		</action>
		<!-- ajax加载修复信息选择页面 -->
		<action name="HrmisRepairinfo_ACTION_ALERT" >
			<result>/hrmis/relic/pHrmisRepairinfoGrid.jsp</result>
		</action>
	</package>
</struts>