<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      标本化石文物信息<include file="org/bkgd/hrmis/relic/web/conf/HrmisRelicFossilWeb.xml"></include> -->
<struts>
	<!-- 标本化石文物信息 -->
	<package name="HrmisRelicFossil_Ajax_code" extends="json-default">
		<!-- 标本化石文物信息查询 -->
		<action name="HrmisRelicFossilqueryAll" method="queryall"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicFossilQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 标本化石文物信息增加 -->
		<action name="HrmisRelicFossiladdCommit" method="addSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicFossilQuery">
			<result type="json"></result>
		</action>
		<!-- 标本化石文物信息删除 -->
		<action name="HrmisRelicFossildeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicFossilQuery">
			<result type="json"></result>
		</action>
		<!-- 标本化石文物信息修改 -->
		<action name="HrmisRelicFossileditCommit" method="editSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicFossilQuery">
			<result type="json"></result>
		</action>
	</package>
	<package name="HrmisRelicFossil_Jsp_code" extends="struts-default">
		<!-- 集成界面 -->
		<action name="HrmisRelicFossilBaseshow" method="view"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicFossilQuery">
			<result>/hrmis/relic/pHrmisRelicFossilBase.jsp</result>
		</action>
		<!-- 标本化石文物信息详细【修改、浏览】 -->
		<action name="HrmisRelicFossilshow" method="view"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicFossilQuery">
			<result>/hrmis/relic/pHrmisRelicFossilEntity.jsp</result>
		</action>
		<!-- html加载标本化石文物信息控制台 -->
		<action name="HrmisRelicFossil_ACTION_CONSOLE">
			<result>/hrmis/relic/pHrmisRelicFossilLayout.jsp</result>
		</action>
		<!-- ajax加载标本化石文物信息选择页面 -->
		<action name="HrmisRelicFossil_ACTION_ALERT" >
			<result>/hrmis/relic/pHrmisRelicFossilGrid.jsp</result>
		</action>
		<!-- html加载标本化石文物信息控制台-->
		<action name="exportExcelRelicFossil" method="report"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicFossilQuery">
			<result type="stream">
				<param name="contentType">application/octet-stream</param>
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">attachment;filename="log.xls"</param>
				<param name="bufferSize">4096</param>
			</result>
		</action>
	</package>
</struts>