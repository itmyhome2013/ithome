<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      文物<include file="org/bkgd/hrmis/relic/web/conf/HrmisRelicWeb.xml"></include> -->
<struts>
	<!-- 文物 -->
	<package name="HrmisRelic_Ajax_code" extends="json-default">
		<!-- 文物查询 -->
		<action name="HrmisRelicqueryAll" method="queryall"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 文物增加 -->
		<action name="HrmisRelicaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicQuery">
			<result type="json"></result>
		</action>
		<!-- 文物删除 -->
		<action name="HrmisRelicdeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicQuery">
			<result type="json"></result>
		</action>
		<!-- 文物修改 -->
		<action name="HrmisReliceditCommit" method="editSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicQuery">
			<result type="json"></result>
		</action>
	</package>
	<package name="HrmisRelic_Jsp_code" extends="struts-default">
		<!-- 文物详细【修改、浏览】 -->
		<action name="HrmisRelicshow" method="view"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicQuery">
			<result>/hrmis/relic/pHrmisRelicEntity.jsp</result>
		</action>
		<!-- html加载文物控制台 -->
		<action name="HrmisRelic_ACTION_CONSOLE">
			<result>/hrmis/relic/pHrmisRelicLayout.jsp</result>
		</action>
		<!-- ajax加载文物选择页面 -->
		<action name="HrmisRelic_ACTION_ALERT" >
			<result>/hrmis/relic/pHrmisRelicGrid.jsp</result>
		</action>
	</package>
</struts>