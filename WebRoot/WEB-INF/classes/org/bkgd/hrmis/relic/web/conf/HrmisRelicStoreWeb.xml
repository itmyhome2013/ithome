<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      保管信息<include file="org/bkgd/hrmis/relic/web/conf/HrmisRelicStoreWeb.xml"></include> -->
<struts>
	<!-- 保管信息 -->
	<package name="HrmisRelicStore_Ajax_code" extends="json-default">
		<!-- 保管信息查询 -->
		<action name="HrmisRelicStorequeryAll" method="queryall"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicStoreQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 保管信息增加 -->
		<action name="HrmisRelicStoreaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicStoreQuery">
			<result type="json"></result>
		</action>
		<!-- 保管信息删除 -->
		<action name="HrmisRelicStoredeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicStoreQuery">
			<result type="json"></result>
		</action>
		<!-- 保管信息修改 -->
		<action name="HrmisRelicStoreeditCommit" method="editSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicStoreQuery">
			<result type="json"></result>
		</action>
	</package>
	<package name="HrmisRelicStore_Jsp_code" extends="struts-default">
		<!-- 保管信息详细【修改、浏览】 -->
		<action name="HrmisRelicStoreshow" method="view"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicStoreQuery">
			<result>/hrmis/relic/pHrmisRelicStoreEntity.jsp</result>
		</action>
		<!-- html加载保管信息控制台 -->
		<action name="HrmisRelicStore_ACTION_CONSOLE">
			<result>/hrmis/relic/pHrmisRelicStoreLayout.jsp</result>
		</action>
		<!-- ajax加载保管信息选择页面 -->
		<action name="HrmisRelicStore_ACTION_ALERT" >
			<result>/hrmis/relic/pHrmisRelicStoreGrid.jsp</result>
		</action>
	</package>
</struts>