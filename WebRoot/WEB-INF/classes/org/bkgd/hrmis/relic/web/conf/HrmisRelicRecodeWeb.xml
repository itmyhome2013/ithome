<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      著录信息<include file="org/bkgd/hrmis/relic/web/conf/HrmisRelicRecodeWeb.xml"></include> -->
<struts>
	<!-- 著录信息 -->
	<package name="HrmisRelicRecode_Ajax_code" extends="json-default">
		<!-- 著录信息查询 -->
		<action name="HrmisRelicRecodequeryAll" method="queryall"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicRecodeQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 著录信息增加 -->
		<action name="HrmisRelicRecodeaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicRecodeQuery">
			<result type="json"></result>
		</action>
		<!-- 著录信息删除 -->
		<action name="HrmisRelicRecodedeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicRecodeQuery">
			<result type="json"></result>
		</action>
		<!-- 著录信息修改 -->
		<action name="HrmisRelicRecodeeditCommit" method="editSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicRecodeQuery">
			<result type="json"></result>
		</action>
	</package>
	<package name="HrmisRelicRecode_Jsp_code" extends="struts-default">
		<!-- 著录信息详细【修改、浏览】 -->
		<action name="HrmisRelicRecodeshow" method="view"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicRecodeQuery">
			<result>/hrmis/relic/pHrmisRelicRecodeEntity.jsp</result>
		</action>
		<!-- html加载著录信息控制台 -->
		<action name="HrmisRelicRecode_ACTION_CONSOLE">
			<result>/hrmis/relic/pHrmisRelicRecodeLayout.jsp</result>
		</action>
		<!-- ajax加载著录信息选择页面 -->
		<action name="HrmisRelicRecode_ACTION_ALERT" >
			<result>/hrmis/relic/pHrmisRelicRecodeGrid.jsp</result>
		</action>
	</package>
</struts>