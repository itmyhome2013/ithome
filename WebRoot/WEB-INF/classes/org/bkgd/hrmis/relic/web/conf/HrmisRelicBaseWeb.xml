<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      基本信息<include file="org/bkgd/hrmis/relic/web/conf/HrmisRelicBaseWeb.xml"></include> -->
<struts>
	<!-- 基本信息 -->
	<package name="HrmisRelicBase_Ajax_code" extends="json-default">
		<!-- 基本信息查询 -->
		<action name="HrmisRelicBasequeryAll" method="queryall"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicBaseQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 基本信息增加 -->
		<action name="HrmisRelicBaseaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicBaseQuery">
			<result type="json"></result>
		</action>
		<!-- 基本信息删除 -->
		<action name="HrmisRelicBasedeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicBaseQuery">
			<result type="json"></result>
		</action>
		<!-- 基本信息修改 -->
		<action name="HrmisRelicBaseeditCommit" method="editSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicBaseQuery">
			<result type="json"></result>
		</action>
	</package>
	<package name="HrmisRelicBase_Jsp_code" extends="struts-default">
		<!-- 基本信息详细【修改、浏览】 -->
		<action name="HrmisRelicBaseshow" method="view"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisRelicBaseQuery">
			<result>/hrmis/relic/pHrmisRelicBaseEntity.jsp</result>
		</action>
		<!-- html加载基本信息控制台 -->
		<action name="HrmisRelicBase_ACTION_CONSOLE">
			<result>/hrmis/relic/pHrmisRelicBaseLayout.jsp</result>
		</action>
		<!-- ajax加载基本信息选择页面 -->
		<action name="HrmisRelicBase_ACTION_ALERT" >
			<result>/hrmis/relic/pHrmisRelicBaseGrid.jsp</result>
		</action>
	</package>
</struts>