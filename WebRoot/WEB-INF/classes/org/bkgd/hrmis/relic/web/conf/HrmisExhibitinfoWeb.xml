<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      展出信息<include file="org/bkgd/hrmis/relic/web/conf/HrmisExhibitinfoWeb.xml"></include> -->
<struts>
	<!-- 展出信息 -->
	<package name="HrmisExhibitinfo_Ajax_code" extends="json-default">
		<!-- 展出信息查询 -->
		<action name="HrmisExhibitinfoqueryAll" method="queryall"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisExhibitinfoQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 展出信息增加 -->
		<action name="HrmisExhibitinfoaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisExhibitinfoQuery">
			<result type="json"></result>
		</action>
		<!-- 展出信息删除 -->
		<action name="HrmisExhibitinfodeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisExhibitinfoQuery">
			<result type="json"></result>
		</action>
		<!-- 展出信息修改 -->
		<action name="HrmisExhibitinfoeditCommit" method="editSubmit"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisExhibitinfoQuery">
			<result type="json"></result>
		</action>
	</package>
	<package name="HrmisExhibitinfo_Jsp_code" extends="struts-default">
		<!-- 展出信息详细【修改、浏览】 -->
		<action name="HrmisExhibitinfoshow" method="view"
			class="org.bkgd.hrmis.relic.web.action.ActionHrmisExhibitinfoQuery">
			<result>/hrmis/relic/pHrmisExhibitinfoEntity.jsp</result>
		</action>
		<!-- html加载展出信息控制台 -->
		<action name="HrmisExhibitinfo_ACTION_CONSOLE">
			<result>/hrmis/relic/pHrmisExhibitinfoLayout.jsp</result>
		</action>
		<!-- ajax加载展出信息选择页面 -->
		<action name="HrmisExhibitinfo_ACTION_ALERT" >
			<result>/hrmis/relic/pHrmisExhibitinfoGrid.jsp</result>
		</action>
	</package>
</struts>