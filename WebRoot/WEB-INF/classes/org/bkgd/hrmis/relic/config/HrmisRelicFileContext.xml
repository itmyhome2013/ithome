<?xml version="1.0" encoding="UTF-8"?>
<!--将该脚本加入spring配置文件中      文物附件<import resource="classpath:org/bkgd/hrmis/relic/config/HrmisRelicFileContext.xml"></import> -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<!-- 文物附件Dao实现 -->
	<bean id="HRMIS_RELIC_FILEDaoId"
		class="org.bkgd.hrmis.relic.dao.HrmisRelicFileDao">
		<property name="sessionFatory">
			<ref bean="sessionFactory" />
		</property>
		<property name="sqlTools">
			<bean id="HrmisRelicFile_sqlTool" class="com.farm.core.sql.utils.HibernateSQLTools">
				<constructor-arg>
					<ref bean="sessionFactory" />
				</constructor-arg>
				<constructor-arg>
					<bean id="HrmisRelicFile_object" class="org.bkgd.hrmis.relic.domain.HrmisRelicFile"></bean>
				</constructor-arg>
			</bean>
		</property>
	</bean>
	<!-- Server实现 -->
	<bean id="HRMIS_RELIC_FILEServerId"
		class="org.bkgd.hrmis.relic.server.HrmisRelicFileManagerImpl">
		<property name="hrmisRelicFileDao">
			<ref bean="HRMIS_RELIC_FILEDaoId" />
		</property>
	</bean>
	<!-- Server事务代理类 -->
	<bean id="HRMIS_RELIC_FILEProxyId"
		class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean">
		<property name="proxyInterfaces">
			<list>
				<value>
					org.bkgd.hrmis.relic.server.HrmisRelicFileManagerInter
				</value>
			</list>
		</property>
		<property name="transactionManager">
			<ref bean="TransactionManager" />
		</property>
		<property name="target">
			<ref bean="HRMIS_RELIC_FILEServerId" />
		</property>
		<property name="transactionAttributeSource">
			<ref bean="KEYHRMIS_RELIC_FILETRANCACTION" />
		</property>
	</bean>
	<!-- 事务属性声明 -->
	<bean id="KEYHRMIS_RELIC_FILETRANCACTION"
		class="org.springframework.transaction.interceptor.NameMatchTransactionAttributeSource">
		<property name="properties">
			<props>
				<prop key="*">PROPAGATION_REQUIRED</prop>
			</props>
		</property>
	</bean>
</beans>