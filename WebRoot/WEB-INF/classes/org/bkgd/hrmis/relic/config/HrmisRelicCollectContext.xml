<?xml version="1.0" encoding="UTF-8"?>
<!--将该脚本加入spring配置文件中      流传经历<import resource="classpath:org/bkgd/hrmis/relic/config/HrmisRelicCollectContext.xml"></import> -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<!-- 流传经历Dao实现 -->
	<bean id="HRMIS_RELIC_COLLECTDaoId"
		class="org.bkgd.hrmis.relic.dao.HrmisRelicCollectDao">
		<property name="sessionFatory">
			<ref bean="sessionFactory" />
		</property>
		<property name="sqlTools">
			<bean id="HrmisRelicCollect_sqlTool" class="com.farm.core.sql.utils.HibernateSQLTools">
				<constructor-arg>
					<ref bean="sessionFactory" />
				</constructor-arg>
				<constructor-arg>
					<bean id="HrmisRelicCollect_object" class="org.bkgd.hrmis.relic.domain.HrmisRelicCollect"></bean>
				</constructor-arg>
			</bean>
		</property>
	</bean>
	<!-- Server实现 -->
	<bean id="HRMIS_RELIC_COLLECTServerId"
		class="org.bkgd.hrmis.relic.server.HrmisRelicCollectManagerImpl">
		<property name="hrmisRelicCollectDao">
			<ref bean="HRMIS_RELIC_COLLECTDaoId" />
		</property>
	</bean>
	<!-- Server事务代理类 -->
	<bean id="HRMIS_RELIC_COLLECTProxyId"
		class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean">
		<property name="proxyInterfaces">
			<list>
				<value>
					org.bkgd.hrmis.relic.server.HrmisRelicCollectManagerInter
				</value>
			</list>
		</property>
		<property name="transactionManager">
			<ref bean="TransactionManager" />
		</property>
		<property name="target">
			<ref bean="HRMIS_RELIC_COLLECTServerId" />
		</property>
		<property name="transactionAttributeSource">
			<ref bean="KEYHRMIS_RELIC_COLLECTTRANCACTION" />
		</property>
	</bean>
	<!-- 事务属性声明 -->
	<bean id="KEYHRMIS_RELIC_COLLECTTRANCACTION"
		class="org.springframework.transaction.interceptor.NameMatchTransactionAttributeSource">
		<property name="properties">
			<props>
				<prop key="*">PROPAGATION_REQUIRED</prop>
			</props>
		</property>
	</bean>
</beans>