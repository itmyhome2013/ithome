<?xml version="1.0" encoding="UTF-8"?>
<!--将该脚本加入spring配置文件中      鉴定信息<import resource="classpath:org/bkgd/hrmis/relic/config/HrmisRelicAuthenticateContext.xml"></import> -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<!-- 鉴定信息Dao实现 -->
	<bean id="HRMIS_RELIC_AUTHENTICATEDaoId"
		class="org.bkgd.hrmis.relic.dao.HrmisRelicAuthenticateDao">
		<property name="sessionFatory">
			<ref bean="sessionFactory" />
		</property>
		<property name="sqlTools">
			<bean id="HrmisRelicAuthenticate_sqlTool" class="com.farm.core.sql.utils.HibernateSQLTools">
				<constructor-arg>
					<ref bean="sessionFactory" />
				</constructor-arg>
				<constructor-arg>
					<bean id="HrmisRelicAuthenticate_object" class="org.bkgd.hrmis.relic.domain.HrmisRelicAuthenticate"></bean>
				</constructor-arg>
			</bean>
		</property>
	</bean>
	<!-- Server实现 -->
	<bean id="HRMIS_RELIC_AUTHENTICATEServerId"
		class="org.bkgd.hrmis.relic.server.HrmisRelicAuthenticateManagerImpl">
		<property name="hrmisRelicAuthenticateDao">
			<ref bean="HRMIS_RELIC_AUTHENTICATEDaoId" />
		</property>
	</bean>
	<!-- Server事务代理类 -->
	<bean id="HRMIS_RELIC_AUTHENTICATEProxyId"
		class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean">
		<property name="proxyInterfaces">
			<list>
				<value>
					org.bkgd.hrmis.relic.server.HrmisRelicAuthenticateManagerInter
				</value>
			</list>
		</property>
		<property name="transactionManager">
			<ref bean="TransactionManager" />
		</property>
		<property name="target">
			<ref bean="HRMIS_RELIC_AUTHENTICATEServerId" />
		</property>
		<property name="transactionAttributeSource">
			<ref bean="KEYHRMIS_RELIC_AUTHENTICATETRANCACTION" />
		</property>
	</bean>
	<!-- 事务属性声明 -->
	<bean id="KEYHRMIS_RELIC_AUTHENTICATETRANCACTION"
		class="org.springframework.transaction.interceptor.NameMatchTransactionAttributeSource">
		<property name="properties">
			<props>
				<prop key="*">PROPAGATION_REQUIRED</prop>
			</props>
		</property>
	</bean>
</beans>