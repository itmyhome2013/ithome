<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      文物出入库信息<include file="org/bkgd/hrmis/app/web/conf/HrmisAppRecrelicWeb.xml"></include>
-->
<struts>
	<!-- 移动文物出入库信息 -->
	<package name="HrmisAppRecrelicMoveInfo_Ajax_code" extends="json-default">
		<!-- 移动文物出入库信息查询 -->
		<action name="HrmisAppRecrelicMoveInfoqueryAll" method="queryall"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicMoveInfoQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 文物出入库信息删除 -->
		<action name="HrmisAppRecrelicMoveInfodeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicMoveInfoQuery">
			<result type="json"></result>
		</action>
		<!-- 移动文物出入库信息增加 -->
		<action name="HrmisAppRecrelicMoveInfoaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicMoveInfoQuery">
			<result type="json"></result>
		</action>
		<!-- 文物查询 -->
		<action name="HrmisAppRecrelicMoveinfoRelicqueryAll" method="moveInfoRelicQueryAll"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicMoveInfoQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 选择文物 -->
		<action name="HrmisAppRecrelicMoveInfoSubmitRelic" method="submitRelic"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicMoveInfoQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 删除所选文物 -->
		<action name="HrmisAppRecrelicMoveInfodeleteRelicCommit" method="delRelic"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicMoveInfoQuery">
			<result type="json"></result>
		</action>
		
		<!-- 查询移动出入库所选文物  -->
		<action name="HrmisAppRecrelicMoveInfoChooseRelic" method="chooseRelicQueryall"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicMoveInfoQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		
	</package>
	<package name="HrmisAppRecrelicMoveInfo_Jsp_code" extends="struts-default">
	
		<!-- html加载文物出入库信息控制台 -->
		<action name="HrmisAppRecrelicMoveInfo_ACTION_CONSOLE">
			<result>/hrmis/app/moveinfo/pHrmisMoveInfoLayout.jsp</result>
		</action>
		<!-- 文物出入库信息详细【修改、浏览】 -->
		<action name="HrmisAppRecrelicMoveInfoshow" method="view"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicMoveInfoQuery">
			<result>/hrmis/app/moveinfo/pHrmisMoveInfoEntity.jsp</result>
		</action>
		<!-- 添加文物 -->
		<action name="HrmisAppRecrelicMoveInfoAddRelic" method="forSend"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicMoveInfoQuery">
			<result>/hrmis/app/moveinfo/pHrmisMoveInfoAddRelicLayout.jsp</result>
		</action>
		<!-- 导出  -->
		<action name="HrmisRelicMoveInfoReport" method="report"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicMoveInfoQuery">
			<result type="stream">
				<param name="contentType">application/octet-stream</param>
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">attachment;filename="recrelicMoveInfo.xls"</param>
				<param name="bufferSize">4096</param>
			</result>
		</action>
	</package>
</struts>