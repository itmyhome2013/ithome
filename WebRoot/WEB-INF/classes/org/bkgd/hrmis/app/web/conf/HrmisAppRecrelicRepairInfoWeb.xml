<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      文物出入库信息<include file="org/bkgd/hrmis/app/web/conf/HrmisAppRecrelicWeb.xml"></include>
-->
<struts>
	<!-- 修复文物出入库信息 -->
	<package name="HrmisAppRecrelicRepairInfo_Ajax_code" extends="json-default">
		<!-- 修复文物出入库信息查询 -->
		<action name="HrmisAppRecrelicRepairInfoqueryAll" method="queryall"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicRepairInfoQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 文物出入库信息删除 -->
		<action name="HrmisAppRecrelicRepairInfodeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicRepairInfoQuery">
			<result type="json"></result>
		</action>
		<!-- 修复文物出入库信息增加 -->
		<action name="HrmisAppRecrelicRepairInfoaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicRepairInfoQuery">
			<result type="json"></result>
		</action>
		<!-- 文物查询 -->
		<action name="HrmisAppRecrelicRepairinfoRelicqueryAll" method="moveInfoRelicQueryAll"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicRepairInfoQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 选择文物 -->
		<action name="HrmisAppRecrelicRepairInfoSubmitRelic" method="submitRelic"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicRepairInfoQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 删除所选文物 -->
		<action name="HrmisAppRecrelicRepairInfodeleteRelicCommit" method="delRelic"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicRepairInfoQuery">
			<result type="json"></result>
		</action>
		<!-- 查询修复出入库所选文物  -->
		<action name="HrmisAppRecrelicRepairInfoChooseRelic" method="chooseRelicQueryall"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicRepairInfoQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		
	</package>
	<package name="HrmisAppRecrelicRepairInfo_Jsp_code" extends="struts-default">
	
		<!-- html加载文物出入库信息控制台 -->
		<action name="HrmisAppRecrelicRepairInfo_ACTION_CONSOLE">
			<result>/hrmis/app/repairinfo/pHrmisRepairInfoLayout.jsp</result>
		</action>
		<!-- 文物出入库信息详细【修改、浏览】 -->
		<action name="HrmisAppRecrelicRepairInfoshow" method="view"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicRepairInfoQuery">
			<result>/hrmis/app/repairinfo/pHrmisRepairInfoEntity.jsp</result>
		</action>
		<!-- 添加文物 -->
		<action name="HrmisAppRecrelicRepairInfoAddRelic" method="forSend"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicRepairInfoQuery">
			<result>/hrmis/app/repairinfo/pHrmisRepairInfoAddRelicLayout.jsp</result>
		</action>
		<!-- 导出  -->
		<action name="HrmisRelicRepairInfoReport" method="report"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicRepairInfoQuery">
			<result type="stream">
				<param name="contentType">application/octet-stream</param>
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">attachment;filename="recrelicRepairInfo.xls"</param>
				<param name="bufferSize">4096</param>
			</result>
		</action>
	</package>
</struts>