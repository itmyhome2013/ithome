<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<!--将该脚本加入struts2配置文件中      文物出入库信息<include file="org/bkgd/hrmis/app/web/conf/HrmisAppRecrelicWeb.xml"></include> -->
<struts>
	<!-- 文物出入库信息 -->
	<package name="HrmisAppRecrelic_Ajax_code" extends="json-default">
		<!-- 文物出入库信息查询 -->
		<action name="HrmisAppRecrelicqueryAll" method="queryall"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 文物出入库信息增加 -->
		<action name="HrmisAppRecrelicaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicQuery">
			<result type="json"></result>
		</action>
		<!-- 文物出入库信息删除 -->
		<action name="HrmisAppRecrelicdeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicQuery">
			<result type="json"></result>
		</action>
		<!-- 文物出入库信息修改 -->
		<action name="HrmisAppRecreliceditCommit" method="editSubmit"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicQuery">
			<result type="json"></result>
		</action>
	</package>
	<package name="HrmisAppRecrelic_Jsp_code" extends="struts-default">
		<!-- 文物出入库信息详细【修改、浏览】 -->
		<action name="HrmisAppRecrelicshow" method="view"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppRecrelicQuery">
			<result>/hrmis/app/pHrmisAppRecrelicEntity.jsp</result>
		</action>
		<!-- html加载文物出入库信息控制台 -->
		<action name="HrmisAppRecrelic_ACTION_CONSOLE">
			<result>/hrmis/app/pHrmisAppRecrelicLayout.jsp</result>
		</action>
		<!-- ajax加载文物出入库信息选择页面 -->
		<action name="HrmisAppRecrelic_ACTION_ALERT" >
			<result>/hrmis/app/pHrmisAppRecrelicGrid.jsp</result>
		</action>
	</package>
</struts>