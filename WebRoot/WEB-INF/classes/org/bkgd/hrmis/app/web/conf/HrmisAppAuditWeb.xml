<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
	<!--
		将该脚本加入struts2配置文件中 审核信息<include
		file="org/bkgd/hrmis/app/web/conf/HrmisAppAuditWeb.xml"></include>
	-->
<struts>
	<!-- 审核信息 -->
	<package name="HrmisAppAudit_Ajax_code" extends="json-default">
		<!-- 审核信息查询 -->
		<action name="HrmisAppAuditqueryAll" method="queryall"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppAuditQuery">
			<result type="json">
				<param name="root">jsonResult</param>
			</result>
		</action>
		<!-- 审核信息增加 -->
		<action name="HrmisAppAuditaddCommit" method="addSubmit"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppAuditQuery">
			<result type="json"></result>
		</action>
		<!-- 审核信息删除 -->
		<action name="HrmisAppAuditdeleteCommit" method="delSubmit"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppAuditQuery">
			<result type="json"></result>
		</action>
		<!-- 审核信息修改 -->
		<action name="HrmisAppAuditeditCommit" method="editSubmit"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppAuditQuery">
			<result type="json"></result>
		</action>
	</package>
	<package name="HrmisAppAudit_Jsp_code" extends="struts-default">
		<!-- 审核信息详细【修改、浏览】 -->
		<action name="HrmisAppAuditshow" method="view"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppAuditQuery">
			<result>/hrmis/app/pHrmisAppAuditEntity.jsp</result>
		</action>
		<!-- 审核业务的详细信息 1盘点、2移动出入库、3修复出入库、4展出出入库、5其他出入库-->
		<action name="HrmisAppAuditAppShow" method="appView"
			class="org.bkgd.hrmis.app.web.action.ActionHrmisAppAuditQuery">
			<result>/hrmis/app/pHrmisAppAuditNoAppMessage.jsp</result>
			<result name="type1" type="redirect">
				admin/HrmisAppCheckrelic_ACTION_CONSOLE_VIEW.do?pageset.pageType=0&amp;ids=${ids}
			</result>
			<result name="type2" type="redirect">
				admin/HrmisAppRecrelicMoveInfoshow.do?pageset.pageType=0&amp;ids=${ids}
			</result>
			<result name="type3" type="redirect">
				admin/HrmisAppRecrelicRepairInfoshow.do?pageset.pageType=0&amp;ids=${ids}
			</result>
			<result name="type4" type="redirect">
				admin/HrmisAppRecrelicExhibitInfoshow.do?pageset.pageType=0&amp;ids=${ids}
			</result>
			<result name="type5" type="redirect">admin/HrmisAppRecrelicMoveInfoshow.do?pageset.pageType=0&amp;ids=${ids}
			</result>
		</action>
		<!-- html加载审核信息管理 -->
		<action name="HrmisAppAudit_ACTION_CONSOLE">
			<result>/hrmis/app/pHrmisAppAuditLayout.jsp</result>
		</action>
		<!-- html加载审核信息历史查询 -->
		<action name="HrmisAppAudit_HIS_CONSOLE">
			<result>/hrmis/app/pHrmisAppAuditHistoryLayout.jsp</result>
		</action>
		<!-- ajax加载审核信息选择页面 -->
		<action name="HrmisAppAudit_ACTION_ALERT">
			<result>/hrmis/app/pHrmisAppAuditGrid.jsp</result>
		</action>
	</package>
</struts>