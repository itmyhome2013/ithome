<?xml version="1.0" encoding="UTF-8"?>
<!--将该脚本加入spring配置文件中      展出照片资料<import resource="classpath:org/bkgd/hrmis/app/config/HrmisAppExhibitfileContext.xml"></import> -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<!-- 展出照片资料Dao实现 -->
	<bean id="HRMIS_APP_EXHIBITFILEDaoId"
		class="org.bkgd.hrmis.app.dao.HrmisAppExhibitfileDao">
		<property name="sessionFatory">
			<ref bean="sessionFactory" />
		</property>
		<property name="sqlTools">
			<bean id="HrmisAppExhibitfile_sqlTool" class="com.farm.core.sql.utils.HibernateSQLTools">
				<constructor-arg>
					<ref bean="sessionFactory" />
				</constructor-arg>
				<constructor-arg>
					<bean id="HrmisAppExhibitfile_object" class="org.bkgd.hrmis.app.domain.HrmisAppExhibitfile"></bean>
				</constructor-arg>
			</bean>
		</property>
	</bean>
</beans>